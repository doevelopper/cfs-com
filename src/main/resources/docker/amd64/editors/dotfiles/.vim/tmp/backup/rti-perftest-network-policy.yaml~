apiVersion: networking.k8s.io/v1
kind: NetworkPolicy
metadata:
  name: "rti-perftest-1234"
  namespace: itvl-buc-adrien
spec:
  podSelector:
    matchLabels:
      app: rti-perftest
      id: "1234"
  policyTypes:
  - Ingress
  - Egress
  ingress:
  - from:
    - podSelector:
        matchLabels:
          app: rti-perftest
          id: "1234"
  egress:
  - to:
    - podSelector:
        matchLabels:
          app: rti-perftest
          id: "1234"
