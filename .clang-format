---
Language:        Cpp
AccessModifierOffset: -2
ColumnLimit: 120
DerivePointerAlignment: false
PointerAlignment: Left
SortIncludes: false
Standard: Cpp17
UseTab: Never
IndentWidth: '4'
ForEachMacros:   [ foreach, Q_FOREACH, BOOST_FOREACH ]
IncludeCategories:
  - Regex:           '^<.*\.h>'
    Priority:        1
  - Regex:           '^<.*'
    Priority:        2
  - Regex:           '.*'
    Priority:        3
IncludeIsMainRegex: '([-_](test|unittest))?$'
...

